{
    "name": "angusm73/unoconv",
    "description": "Convert documents using unoconv inside a Laravel application.",
    "keywords": [
        "converter",
        "documents",
        "unoconv",
        "laravel"
    ],
    "homepage": "https://github.com/angusm73/unoconv",
    "license": "MIT",
    "authors": [
        {
            "name": "Angus Moore",
            "email": "hi@angus.digital",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Angusm73\\Unoconv\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Angusm73\\Unoconv\\UnoconvServiceProvider"
            ],
            "aliases": {
                "Unoconv": "Angusm73\\Unoconv\\Facades\\Unoconv"
            }
        }
    }
}
